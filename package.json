{
  "private": true,
  "name": "tfractal-playground",
  "version": "1.0.0",
  "description": "Playground for the react-tfractal component",
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "prod": "better-npm-run prod",
    "server": "babel src/server.js --out-file dist/server.js --babelrc=false --plugins=transform-async-to-generator"
  },
  "betterScripts": {
    "start": {
      "command": "node dist/server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "dev": {
      "command": "node devServer/devServer.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "prod": {
      "command": "webpack --display-error-details --progress --config webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8080,
        "UV_THREADPOOL_SIZE": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aush/tfractal-playground.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "fractal",
    "t-fractal",
    "tfractal"
  ],
  "author": {
    "name": "Anton Matveev",
    "email": "aush.tx@gmail.com",
    "url": "http://github.com/aush"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aush/tfractal-playground/issues"
  },
  "homepage": "https://github.com/aush/tfractal-playground",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "nib": "^1.1.0",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1",
    "write-file-webpack-plugin": "^3.1.7"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "color": "^0.11.1",
    "koa": "^2.0.0-alpha.3",
    "koa-send": "^3.1.0",
    "lodash": "^4.5.0",
    "material-ui": "^0.14.4",
    "react": "^0.14.7",
    "react-4color-picker": "^1.0.5",
    "react-center": "^1.1.1",
    "react-css-modules": "^3.7.4",
    "react-dom": "^0.14.7",
    "react-rectangle": "^1.3.3",
    "react-redux": "^4.4.0",
    "react-stack": "^1.2.2",
    "react-tfractal": "^1.1.5",
    "recompose": "^0.15.0",
    "redux": "^3.3.1"
  },
  "engines": {
    "node": ">=5.2.0"
  }
}
